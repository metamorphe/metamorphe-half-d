<div class="row">
	<h3 class="pull-left">Swatch Library</h3>
	<%= link_to new_swatch_path, :class=> "pull-right btn btn-lg btn-danger" do %>
		<span class="glyphicon glyphicon-upload"></span>
		New Swatch
	<% end %>
</div>
<!-- <br class="clearfix"> -->

<div class="row">
   <% @swatches.reverse.each do |swatch| %>
      <%= render :partial => "node", :locals => {:swatch => swatch} %>
    <% end %>
</div>

<br>
